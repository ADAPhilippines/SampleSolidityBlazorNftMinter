pragma solidity ^0.8.9;
import "./CustomNFT.sol";

contract Minter {
    CustomNFT public NFT;

    constructor() {
        NFT = new CustomNFT();
    }

    function mint() payable public {
        require(msg.value == 1 ether, "Must pay 1 ADA");
        NFT.safeMint(msg.sender, "data:application/json;base64,");
    }
}
